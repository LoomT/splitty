<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.*?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.Cursor?>
<?import javafx.scene.layout.*?>
<fx:root alignment="CENTER_LEFT" focusTraversable="true" onMouseClicked="#onEventClicked" prefHeight="35.0" prefWidth="355.0" styleClass="eventListItemContainer" stylesheets="@EventListItem.css" type="javafx.scene.layout.HBox" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1">
   <children>
      <Label fx:id="eventTitleLabel" prefHeight="17.0" prefWidth="199.0" text="Label" HBox.hgrow="ALWAYS">
         <HBox.margin>
            <Insets right="10.0" />
         </HBox.margin></Label>
      <Label fx:id="eventCodeLabel" prefHeight="17.0" prefWidth="77.0" text="Code" />
      <Button id="removeEventButton" mnemonicParsing="false" onAction="#onXClick" text="X">
         <opaqueInsets>
            <Insets />
         </opaqueInsets>
         <HBox.margin>
            <Insets left="5.0" right="5.0" />
         </HBox.margin></Button>
      <Button id="downloadEventButton" mnemonicParsing="false" onAction="#onDownloadClick" text="V">
         <HBox.margin>
            <Insets left="5.0" right="5.0" />
         </HBox.margin>
      </Button>
   </children>
   <padding>
      <Insets bottom="5.0" left="10.0" right="10.0" top="5.0" />
   </padding>
   <cursor>
      <Cursor fx:constant="HAND" />
   </cursor>
</fx:root>
